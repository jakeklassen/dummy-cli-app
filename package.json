{
  "name": "dummy-cli-app",
  "version": "0.0.3",
  "description": "Testing my base repo - ignore this.",
  "repository": {
    "type": "git",
    "url": "https://github.com/jakeklassen/dummy-cli-app.git"
  },
  "scripts": {
    "build": "pika build",
    "lint": "tslint --project .",
    "release": "pika publish",
    "prestart": "yarn build",
    "start": "node pkg/dist-node/index.bin.js",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage",
    "version": "pika build",
    "postinstall": "apt-get update"
  },
  "author": "Jake Klassen <jklassendev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@pika/cli": "^0.2.0",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.7.1",
    "@pika/plugin-simple-bin": "^0.7.1",
    "@pika/plugin-ts-standard-pkg": "^0.7.1",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.12",
    "jest": "^24.9.0",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2",
    "typescript-tslint-plugin": "^0.5.5",
    "uglify-js": "^3.6.9"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg",
        {
          "tsconfig": "./tsconfig.build.json"
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-simple-bin",
        {
          "bin": "dummy-cli-app"
        }
      ]
    ]
  },
  "dependencies": {}
}
